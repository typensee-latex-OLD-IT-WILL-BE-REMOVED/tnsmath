\newcounter{@demo@explain@id}


\newcommand\@expl@id[1]{%
    \framebox[1.5em]{\the@demo@explain@id}%
    \if\relax\detokenize{#1}\relax\else%
        \addtocounter{@demo@explain@id}{-1}%
